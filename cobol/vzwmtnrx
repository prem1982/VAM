SAY_MSG:                                                                04040016
arg bl                                                                  04050016
if datatype(msgtext.0) = 'NUM' then ,                                   04060016
   do m=1 to msgtext.0                                                  04070016
      m=right(m,2,0)                                                    04080016
      say msgtext.m                                                     04090016
   end                                                                  04100016
else ,                                                                  04110016
   do                                                                   04120016
      say msgtext                                                       04130016
   end                                                                  04140016
do bl                                                                   04150016
   say ' '                                                              04160016
end                                                                     04170016
drop msgtext.                                                           04180016
RETURN 1                                                                04190016
