then do                                                                 01720016
     SAY 'ERROR OPENING CURSOR C52 - SQLCODE=' SQLCODE                  01730016
     CALL SQLCA                                                         01740016
     exit 12                                                            01750016
     END                                                                01760016
                                                                        01770016
ADDRESS DSNREXX "EXECSQL "CSQL                                          01780016
                                                                        01790016
if SQLCODE = 0                                                          01800016
then do                                                                 01810016
     mtn_found = 'Y'                                                    01820016
     end                                                                01830016
else do                                                                 01831016
     mtn_found = 'N'                                                    01831116
     end                                                                01832016
                                                                        01840016
                                                                        01850016
