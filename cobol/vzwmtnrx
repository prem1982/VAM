 x = "|"                                                                00090016
 numeric digits 10                                                      00100016
                                                                        00110016
 SELECT                                                                 00120016
   WHEN DB2SYS  = 'D001'                                                00130016
     then do                                                            00140016
          TABPREF = 'BMGVZT'                                            00150016
          HILVL   = 'BMGVZ0CN'                                          00151016
          end                                                           00160016
   WHEN DB2SYS  = 'D009'                                                00170016
     then do                                                            00180016
          TABPREF = 'BMGVZP'                                            00190016
