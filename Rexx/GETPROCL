/** GETPROCL: REXX TO CONVERT 8 MEMBER TO LONG STORED PROC NAME **/             
  ARG NAME8                                                                     
  SELECT                                                                        
     WHEN NAME8 = 'BMGS260 ' THEN NAMELONG = 'PORTAL_BILL_INFO'                 
     WHEN NAME8 = 'BMGS270 ' THEN NAMELONG = 'PORTAL_BILL_INFO'                 
     WHEN NAME8 = 'BMGS280 ' THEN NAMELONG = 'PORTAL_IBRS_INFO'                 
     WHEN NAME8 = 'BMGS302 ' THEN NAMELONG = 'PROV_ADJ_LIST'                    
     WHEN NAME8 = 'BMGS303 ' THEN NAMELONG = 'FIND_SHELL_IP'                    
     WHEN NAME8 = 'BMGS919 ' THEN NAMELONG = 'UPD_FREQ_CALLS'                   
     WHEN NAME8 = 'BMGS701 ' THEN NAMELONG = 'BAN_BILL_V42'                     
     WHEN NAME8 = 'BMGS703 ' THEN NAMELONG = 'ENTITY_LOB_CHG_V60'               
     WHEN NAME8 = 'BMGS704 ' THEN NAMELONG = 'BAN_LOB_LIST_V60'                 
     WHEN NAME8 = 'BMGS710 ' THEN NAMELONG = 'SUMMARY_BILL_V52'                 
     WHEN NAME8 = 'BMGS313 ' THEN NAMELONG = 'PAY_ADJ_DETAILS'                  
     WHEN NAME8 = 'BMGS314 ' THEN NAMELONG = 'USAGE_MINUTES'                    
     WHEN NAME8 = 'BMGA300 ' THEN NAMELONG = 'AGG_CNTLSPXT'                     
     WHEN NAME8 = 'BMGA301 ' THEN NAMELONG = 'AGG_CNTLCCXT'                     
     WHEN NAME8 = 'BMGA302 ' THEN NAMELONG = 'AGG_CNTLSCXT'                     
     WHEN NAME8 = 'BMGA303 ' THEN NAMELONG = 'AGG_CALLANST'                     
     WHEN NAME8 = 'BMGA304 ' THEN NAMELONG = 'GRG_CALLANST'                     
     WHEN NAME8 = 'BMGA305 ' THEN NAMELONG = 'AGG_CNTLUSAT'                     
     WHEN NAME8 = 'BMGS300 ' THEN NAMELONG = 'MKTG_REG_MESGS'                   
     WHEN NAME8 = 'BMGS301 ' THEN NAMELONG = 'GET_TN_LIST'                      
     WHEN NAME8 = 'BMGS302 ' THEN NAMELONG = 'GET_BILL_ADDRESS'                 
     WHEN NAME8 = 'BMGS303 ' THEN NAMELONG = 'CARRIER_PIC_RPT'                  
     WHEN NAME8 = 'BMGS304 ' THEN NAMELONG = 'WTN_CIRCUIT_RPT'                  
     WHEN NAME8 = 'BMGS305 ' THEN NAMELONG = 'ITEMIZED_CALL_RPT'                
     WHEN NAME8 = 'BMGS306 ' THEN NAMELONG = 'NON_RECUR_OCC_RPT'                
     WHEN NAME8 = 'BMGS307 ' THEN NAMELONG = 'SBILL_SPREADSHEET'                
     WHEN NAME8 = 'BMGS308 ' THEN NAMELONG = 'WTN_LOB_ENT_LST'                  
     WHEN NAME8 = 'BMGS309 ' THEN NAMELONG = 'ALL_LOAD_DATES'                   
     WHEN NAME8 = 'BMGS310 ' THEN NAMELONG = 'FTP_DATA_BYTES'                   
     WHEN NAME8 = 'BMGS311 ' THEN NAMELONG = 'CALL_SUBTASK'                     
     WHEN NAME8 = 'BMGS500 ' THEN NAMELONG = 'GET_CUSTPRF'                      
     WHEN NAME8 = 'BMGS700 ' THEN NAMELONG = 'GET_SUMMARY_BILL'                 
     WHEN NAME8 = 'BMGS701 ' THEN NAMELONG = 'GET_ENTITY_CHG_LST'               
     WHEN NAME8 = 'BMGS702 ' THEN NAMELONG = 'GET_ENTITY_LOB_LST'               
     WHEN NAME8 = 'BMGS613 ' THEN NAMELONG = 'LOB_RPT_PAGE_V60'                 
     WHEN NAME8 = 'BMGS621 ' THEN NAMELONG = 'LOB_RPT_STAGE_V60'                
     WHEN NAME8 = 'BMGS820 ' THEN NAMELONG = 'LOB_REPORT_DL'                    
     WHEN NAME8 = 'BMGS812 ' THEN NAMELONG = 'REPORT_LINES_DL'                  
     WHEN NAME8 = 'BMGS704 ' THEN NAMELONG = 'BAN_LOB_LIST_V60'                 
    OTHERWISE                                                                   
        NAMELONG = "GET_"NAME8                                                  
  END                                                                           
  RETURN NAMELONG                                                               
